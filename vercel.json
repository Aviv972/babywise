{
    "version": 2,
    "builds": [
        {
            "src": "src/server.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTHONUNBUFFERED": "1"
    }
} 